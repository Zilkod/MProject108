{
  "browser": true,
  "devel": true,
  "esnext": true,
  "strict": true,
  "curly": true,
  "camelcase": true,
  "eqeqeq": true,
  "forin": true,
  "freeze": true,
  "indent": 2,
  "latedef": "nofunc",
  "noarg": true,
  "noempty": true,
  "nonbsp": true,
  "nonew": true,
  "quotmark": "single",
  "undef": true,
  "unused": true,
  "predef": ["-Promise", "-Set", "-Symbol"],
  "globals": {
    "IOWA": true,
    "TweenMax": true,
    "TweenLite": true,
    "Cubic": true,
    "Circ": true,
    "Back": true,
    "Expo": true,
    "Linear": true,
    "Elastic": true,
    "Modernizr": true,
    "require": true,
    "performance": true,
    "module": true,
    "__STATIC_BASE_EXPERIMENT__": true
  }
}
